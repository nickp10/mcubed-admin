{
  "name": "mcubed-admin",
  "version": "2.0.1",
  "description": "An admin tool to help manage data collected by various mCubed modules",
  "bin": "build/server.js",
  "main": "build/server.js",
  "files": [
    "build/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickp10/mcubed-admin.git"
  },
  "author": "nickp10@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickp10/mcubed-admin/issues"
  },
  "homepage": "https://github.com/nickp10/mcubed-admin#readme",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-client": "webpack --config webpack.client.config.js --mode production",
    "build-server": "webpack --config webpack.server.config.js --mode production",
    "prepare": "npm run build",
    "start": "node build/server.js"
  },
  "dependencies": {
    "argv": "0.0.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "jwt-express": "^1.1.0",
    "moment": "^2.22.2",
    "mongodb": "^3.0.10",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/argv": "0.0.4",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/crypto-js": "^3.1.40",
    "@types/express": "^4.16.0",
    "@types/jwt-express": "^1.1.1",
    "@types/mongodb": "^3.0.19",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/uuid": "^3.4.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3"
  }
